{
	"name": "easyweb/laravel-teams-logging",
	"description": "Laravel handler to sending messages to Microsoft Teams using the Incoming Webhook connector",
	"license": "MIT",
	"authors": [
		{
			"name": "Sabar Tampubolon",
			"email": "sabartampubolon@gmail.com"
		}
	],
	"require": {
		"php": "^7.0|^8.0|^8.1",
		"monolog/monolog": "^3.0"
	},
	"require-dev": {
		"ext-curl": "*"
	},
	"autoload": {
		"psr-4": {
			"MargaTampu\\LaravelTeamsLogging\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"MargaTampu\\LaravelTeamsLogging\\LoggerServiceProvider"
			]
		}
	}
}
